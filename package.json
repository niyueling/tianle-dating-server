    "@types/mongoose": "^5.0.18",
    "@types/node": "^10.0.10",
    "@types/redis": "^2.6.0",
    "@types/superagent": "^3.8.1",
    "@types/winston": "^2.3.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.11.1",
    "forever": "^0.15.2",
    "mocha": "^3.0.2",
    "nodemon": "^1.18.11",
    "sinon": "^1.17.4",
    "supertest": "^2.0.0",
    "ts-node": "5.0.1",
    "tslint": "^5.8.0",
    "typescript": "3.7.4"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/**/*.ts"
    ],
    "exclude": [
      "src/test/**/*",
      "typings"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "lcov"
    ],
    "all": true
  }
