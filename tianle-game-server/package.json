{
  "name": "nodejs-server",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "gm-dev": "nodemon --exec ts-node src/gm-tool/server.js",
    "test": "cross-env NODE_ENV=test mocha --opts src/test/mocha.opts -R dot src/test",
    "localTest": "cross-env NODE_ENV=development mocha --opts src/test/mocha.opts --timeout 3600000",
    "build": "tsc || exit 0",
    "start": "cross-env NODE_ENV=production forever start dist/server.js",
    "stop": "forever stop dist/server.js",
    "update": "git pull && npm install && npm run build && npm run stop && npm run start",
    "dev": "nodemon --ext js,ts -i dist --exec ts-node src/server.js",
    "backend-majiang": "nodemon --ext js,ts -i dist --exec ts-node src/backend.majiang.ts",
    "dist-majiang": "cross-env NODE_ENV=dev INSTANCE_ID=majiang nodemon dist/backend.majiang.js",
    "init-server": "ts-node src/bin/setupRoomIds.ts",
    "tsc": "tsc | sed  -Ee \"s/\\(([[:digit:]]+),([[:digit:]]+)\\)/\\:\\1:\\2 /g\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/friend-mahjong/nodejs-server.git"
  },
